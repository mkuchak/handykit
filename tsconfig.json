{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["esnext"],
    "module": "commonjs",
    "types": ["node"],
    "resolveJsonModule": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  }
}
